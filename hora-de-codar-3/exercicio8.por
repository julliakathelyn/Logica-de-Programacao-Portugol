programa {
  funcao inicio() {
    //8 - Ler um valor N e imprimir todos os valores inteiros entre 1 (inclusive) e N (inclusive). Considere que o N 
    //será sempre maior que ZERO. N  é um valor informado pelo usuário


    inteiro n1
    
    escreva("Digite um valor inteiro maior que zero")
    leia(n1)

    

     para (inteiro i = 0; n1 > i; n1--){
      escreva(n1, " ")
    }
      

  }
}