programa {
  funcao inicio() {
    //4 - Faça um algoritmo que calcule e escreva a média aritmética dos números inteiros entre 15 (inclusive) e 
    //100 (inclusive).

    inteiro n1 = 15
    inteiro n2 = 100

    real media  = (((n2 - n1) + 1) * (n2 + n1)) /2

    escreva(media)


  }
}